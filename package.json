{
  "name": "restaurant-autopilot-pro",
  "version": "1.0.0",
  "description": "Professional Restaurant Management & Review Automation Platform",
  "main": "backend/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && nodemon server.js",
    "dev:frontend": "cd frontend && npm start",
    "build": "cd backend && npm install",
    "start": "cd backend && node server.js",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "keywords": ["restaurant", "automation", "reviews", "google-business", "meta"],
  "author": "Kim",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
